Start-Process PowerShell -Verb runAs
echo -----------------------------------------------------------------
echo Scan Disk...
chkdsk /scan c:
echo -----------------------------------------------------------------
echo.
echo -----------------------------------------------------------------
echo Scan Corrupted File System...
SFC /scannow
echo -----------------------------------------------------------------
echo.
echo Restore Healthy...
Dism /online /cleanup-image /restorehealth
echo -----------------------------------------------------------------
echo.
echo Procces Completely...
echo Silakan Restart Terlebih Dahulu...!!
echo.
pause
exit
