

$DownloadURL = 'https://github.com/hinzdc/hinzdc.github.io/blob/main/get/changechrome.vbs'

$FilePath = "$env:TEMP\setdefaultbrowser.vbs"

try {
    Invoke-WebRequest -Uri $DownloadURL -UseBasicParsing -OutFile $FilePath
} catch {
    Write-Error $_
	Return
}

if (Test-Path $FilePath) {
    cmd.exe /c cscript //NoLogo C:\Windows\Temp\setdefaultbrowser.vbs
}
